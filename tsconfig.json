{
  "extends": "@tsconfig/react-native/tsconfig.json",
   "compilerOptions": {
    "esModuleInterop": true,
    "jsx": "react",
    "module": "commonjs",
    "target": "es6",
    "lib": ["es6", "dom"],
    "sourceMap": true,
    "outDir": "./dist",
    "strict": true,
    "noImplicitAny": false,
    "moduleResolution": "node",
    "resolveJsonModule": true
  },
   "include": ["src/**/*","src/types/custom.d.ts",]
}
